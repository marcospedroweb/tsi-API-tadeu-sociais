{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "airbnb-base",
        "prettier"
    ],
    "overrides": [
    ],
    "parserOptions": {
        "ecmaVersion": "latest",
        "sourceType": "module"
    },
    "rules": {
        "import/extensions": [
            "error",
            "ignorePackages",
            {
              "js": "never"
            }
         ]
    },
    "react/jsx-filename-extension": [1, { "extensions": [".js", ".ts", "tsx"] }],
}
